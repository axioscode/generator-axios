{
  "name": "generator-axios",
  "version": "2.0.0",
  "description": "A static site generator for d3 graphics",
  "main": "generators/index.js",
  "scripts": {
    "setup": "npm install -g yo gulp-cli && npm install && npm link",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axioscode/generator-axios"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator",
    "axios",
    "d3",
    "webpack"
  ],
  "author": "Alex Duner <duner@axios.com>, Gerald Rich <gerald@axios.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "slugify": "^1.3.6",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "prettier": "^1.18.2",
    "stylelint-config-standard": "^19.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/generators/",
      "<rootDir>/node_modules/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "output": "./reports/junit.xml"
  }
}
