{
  "name": "<%= meta.slug %>",
  "version": "0.0.1",
  "description": "<%= meta.description %>",
  "main": "src/js/app.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "setup-aws": "pip install awscli && aws init",
    "build": "webpack -p",
    "lint": "estlint src/js/*",
    "publish": "npm run build && aws s3 cp dist s3://<%= meta.s3bucket %>/<%= meta.s3folder %> --recursive --acl public-read",
    "clean": "rm -rf dist"
  },
  "browserslist": [
    "last 2 major versions"
  ],
  "dependencies": {
    "axios-feta": "^0.0.4",
    "d3": "^5.1.0",
    "d3-axis": "^1.0.8",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "pym.js": "^1.3.2"
  },
  "devDependencies": {
    "archieml": "^0.4.2",
    "aws-sdk": "^2.232.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "core-js": "^2.5.5",
    "googleapis": "^30.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
